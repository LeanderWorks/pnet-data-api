{
    "name": "pnet-data-api",
    "version": "1.0.0",
    "description": "This project contains the Java definition of the API for accessing the data stock of the Partner.Net.",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --port 4200 --mode development --open",
        "build": "webpack --mode production",
        "deploy": "npm run download & npm run build & cd dist & git add --all & git commit -m \"Updates gh-pages\" & git pull --rebase & git push & cd ..",
        "install": "git submodule init & git submodule update & cd dist & git checkout gh-pages & git pull --rebase & cd ..:",
        "download": "curl https://data.auto-partner.net/data/v2/api-docs --output src/swagger/pnet-data-api-swagger.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/porscheinformatik/pnet-data-api.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/porscheinformatik/pnet-data-api/issues"
    },
    "homepage": "https://github.com/porscheinformatik/pnet-data-api#readme",
    "devDependencies": {
        "css-loader": "^3.0.0",
        "gulp": "^4.0.2",
        "html-webpack-plugin": "^3.2.0",
        "style-loader": "^0.21.0",
        "swagger-ui-dist": "^3.23.0",
        "webpack": "^4.35.3",
        "webpack-cli": "^3.3.5",
        "webpack-dev-server": "^3.7.2"
    }
}
